{% extends 'partials/_master.twig' %}

{% block main %}
    {% setcontent aboutservices = 'aurora/services' %}
    {% setcontent services = 'services' %}
    <!--Page Header Start-->
    <section class="page-header" style="background-image: url(assets/images/backgrounds/page-header-bg.jpg);">
        <div class="page-header-shape-1"></div>
        <div class="page-header-shape-2"></div>
        <div class="container">
            <div class="page-header__inner">
                <ul class="thm-breadcrumb list-unstyled">
                    <li><a href="{{ path('homepage') }}">Home</a></li>
                    <li><span>.</span></li>
                    <li>{{ aboutservices.title }}</li>
                </ul>
                <h2>{{ aboutservices.title }}</h2>
            </div>
        </div>
    </section>
    <!--Page Header End-->

    <!--Areas of Practice Start-->
    <section class="areas-of-practice">
        <div class="container">
            <div class="areas-of-practice__top">
                <div class="row">
                    <div class="col-xl-6 col-lg-6">
                        <div class="areas-of-practice__top-left">
                            <div class="section-title text-left">
                                <span class="section-title__tagline">Areas of practice</span>
                                <h2 class="section-title__title">{{ aboutservices.caption }}</h2>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-6 col-lg-6">
                        <div class="areas-of-practice__top-right">
                            <p class="areas-of-practice__top-right-text">{{ aboutservices.description }}</p>
                        </div>
                    </div>
                </div>
            </div>
            <div class="areas-of-practice__bottom">
                <div class="row">
                    {% for service in services %}
                        <div class="col-xl-3 col-lg-6 col-md-6">
                            <!--Areas of Practice Single-->
                            <div class="areas-of-practice__single">
                                <div class="areas-of-practice__icon-box">
                                    <div class="areas-of-practice__icon">
                                        <span class="{{ service.iconImage }}"></span>
                                    </div>
                                    <div class="areas-of-practice__title">
                                        <h3><a href="{{ service|link }}">{{ service.title }}</a></h3>
                                    </div>
                                </div>
                                <p class="areas-of-practice__text">{{ service.excerpt }}</p>
                                <a href="{{ service|link }}" class="areas-of-practice__read-more"><i
                                        class="icon-right-arrow"></i></a>
                            </div>
                        </div>
                    {% endfor %}
                </div>
            </div>
        </div>
    </section>
    <!--Areas of Practice End-->

    <!--Brand Two-->
    <section class="brand-one brand-two brand-three">
        <div class="container">
            <div class="brand-border-top"></div>
            <div class="thm-swiper__slider swiper-container" data-swiper-options='{"spaceBetween": 100, "slidesPerView": 5, "autoplay": { "delay": 5000 }, "breakpoints": {
